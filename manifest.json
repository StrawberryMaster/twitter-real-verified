{
    "name": "Bird checked",
    "version": "0.1.0",
    "description": "Identifies verified users against users with blue twitter subscription",
    "manifest_version":3,
    "icons": { "16": "assets/icon16.png",
                "48": "assets/icon48.png",
                "128": "assets/icon128.png" 
    },
    "action":{
      "default_popup":"settingsBox.html"
    },
    "content_scripts": [
      {
        "js": ["contentScript.js"],
        "matches": [
          "https://mobile.twitter.com/*",
          "https://twitter.com/*"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
            "script.js"
        ],
        "matches": [
            "https://mobile.twitter.com/*",
            "https://twitter.com/*"
        ]
      }
  ]
}