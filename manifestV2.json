{
    "name": "Twitter Real Verified",
    "version": "1.2.0",
    "description": "Identifies verified users against users with Twitter Blue subscription",
    "author": "@luiserdef",
    "default_locale": "en",
    "manifest_version":2,
    "permissions": [
      "tabs",
      "i18n"
    ],
    "icons": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png" 
    },
    "browser_action":{
      "default_popup":"popup/popup.html"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "js": ["contentScript.js"],
        "matches": [
          "https://mobile.twitter.com/*",
          "https://twitter.com/*"
        ]
      }
    ],
    "web_accessible_resources": ["script.js"]
  }
